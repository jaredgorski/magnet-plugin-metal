{
  "extends": ["liferay"],
  "env": {
    "node": true,
    "es6": true
  },
  "globals": {
    "__MAGNET_STATE__": true,
    "__MAGNET_ROUTER__": true,
    "__MAGNET_COMPONENT__": true,
    "document": true,
    "IncrementalDOM": true,
    "window": true
  },
  "rules": {
    "indent": 0,
    "no-unused-vars": [2, {"args": "none"}]
  }
}
