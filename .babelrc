{
  "presets": [
    "metal-jsx",
    ["env", {
      "targets": {
        "browsers": [
          "ie 11",
          "last 2 android versions",
          "last 2 chrome versions",
          "last 2 edge versions",
          "last 2 firefox versions",
          "last 2 ios versions",
          "last 2 safari versions"
        ],
        "node": "current"
      }
    }]
  ],
  "plugins": [
    ["transform-runtime", {
      "helpers": false,
      "polyfill": false,
      "regenerator": true,
      "moduleName": "babel-runtime"
    }]
  ]
}
